{
  "name": "incyclist-services",
  "version": "1.7.1",
  "type": "module",
  "peerDependencies": {
    "gd-eventlog": "^0.1.26"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.49.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.35.0",
    "formdata-node": "^6.0.3",
    "jest": "^30.1.3",
    "jsdoc": "^4.0.5",
    "ts-jest": "^29.4.1",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-no-inherit": "^1.6.1",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "buildx": "npm run lint && tsc",
    "build": "tsc",
    "test": "npx jest",
    "test:e2e": "npx jest --coverage -c jest.e2e-config.cjs  --coverageDirectory coverage.e2e",
    "test:unit": "npx jest --coverage -c jest.unit-config.cjs  ",
    "dev": "tsc --watch",
    "docgen": "typedoc --options typedoc.json",
    "docgen1": "typedoc --readme none --excludeProtected --excludePrivate --plugin typedoc-plugin-no-inherit --plugin  typedoc-plugin-markdown src/index.ts",
    "postversion": "git push --tags"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "directories": {
    "example": "./sample",
    "lib": "./src"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "incyclist-devices": "^2.4.13",
    "promise.any": "^2.0.6",
    "semver": "^7.6.3",
    "tcx-builder": "^1.1.1",
    "uuid": "^13.0.0",
    "xml2js": "^0.6.2"
  }
}
