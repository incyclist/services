{
  "name": "incyclist-services",
  "version": "1.4.25",
  "peerDependencies": {
    "gd-eventlog": "^0.1.26"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.30.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "formdata-node": "^6.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.5.0",
    "typedoc-plugin-no-inherit": "^1.5.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "buildx": "npm run lint && tsc",
    "build": "tsc",
    "test": "npx jest",
    "test:e2e": "npx jest --coverage -c jest.e2e-config.js  --coverageDirectory coverage.e2e",
    "test:unit": "npx jest --coverage -c jest.unit-config.js  ",
    "dev": "tsc --watch",
    "docgen": "typedoc --options typedoc.json",
    "docgen1": "typedoc --readme none --excludeProtected --excludePrivate --plugin typedoc-plugin-no-inherit --plugin  typedoc-plugin-markdown src/index.ts",
    "postversion": "git push --tags"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "directories": {
    "example": "./sample",
    "lib": "./src"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "incyclist-devices": "^2.3.14",
    "promise.any": "^2.0.6",
    "semver": "^7.6.3",
    "tcx-builder": "^1.1.1",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2"
  }
}
