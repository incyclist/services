{
  "name": "incyclist-services",
  "version": "1.7.18",
  "peerDependencies": {
    "gd-eventlog": "^0.1.27"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "incyclist-devices": "^3.0.9",
    "promise.any": "^2.0.6",
    "semver": "^7.7.4",
    "tcx-builder": "^1.1.1",
    "uuid": "^13.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@serialport/binding-mock": "^10.2.2",
    "@serialport/bindings-interface": "^1.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "formdata-node": "^6.0.3",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.10.0",
    "typedoc-plugin-no-inherit": "^1.6.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "buildx": "npm run lint && tsc",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "npx jest",
    "test:e2e": "npx jest --coverage -c jest.e2e-config.cjs  --coverageDirectory coverage.e2e",
    "test:unit": "npx jest --coverage -c jest.unit-config.cjs  ",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "docgen": "typedoc --options typedoc.json",
    "docgen1": "typedoc --readme none --excludeProtected --excludePrivate --plugin typedoc-plugin-no-inherit --plugin  typedoc-plugin-markdown src/index.ts",
    "postversion": "git push --tags"
  },
  "files": [
    "lib/"
  ],
  "module": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "directories": {
    "example": "./sample",
    "lib": "./src"
  }
}
